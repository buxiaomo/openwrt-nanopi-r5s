From 492ed1412075dc2e49b9f0630eb8a2fdad03f44b Mon Sep 17 00:00:00 2001
From: GitHub Action <action@github.com>
Date: Fri, 5 Jan 2024 11:32:22 +0800
Subject: [PATCH 2/3] modify lan ip address

---
 package/base-files/files/bin/config_generate | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/package/base-files/files/bin/config_generate b/package/base-files/files/bin/config_generate
index 86f6b09790..c0182f74dd 100755
--- a/package/base-files/files/bin/config_generate
+++ b/package/base-files/files/bin/config_generate
@@ -162,11 +162,11 @@ generate_network() {
 		static)
 			local ipad
 			case "$1" in
-				lan) ipad=${ipaddr:-"192.168.1.1"} ;;
-				*) ipad=${ipaddr:-"192.168.$((addr_offset++)).1"} ;;
+				lan) ipad=${ipaddr:-"172.16.1.1"} ;;
+				*) ipad=${ipaddr:-"172.16.$((addr_offset++)).1"} ;;
 			esac
 
-			netm=${netmask:-"255.255.255.0"}
+			netm=${netmask:-"255.255.0.0"}
 
 			uci -q batch <<-EOF
 				set network.$1.proto='static'
-- 
